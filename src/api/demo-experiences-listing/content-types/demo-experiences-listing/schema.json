      "type": "decimal"
    },
    "reviewCount": {
      "type": "integer"
    },
    "price": {
      "type": "string"
    },
    "maxGuests": {
      "type": "integer"
    },
    "saleOff": {
      "type": "string"
    },
    "map": {
      "type": "component",
      "repeatable": false,
      "component": "map.map"
    },
    "author": {
      "type": "relation",
